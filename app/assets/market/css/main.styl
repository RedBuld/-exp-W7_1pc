@font-face{
    font-family: 'wf_SegoeUINormal';
    src:url('fonts/latest.woff')  format('woff');
    font-weight: normal;
    font-style: normal;
}
@font-face{
    font-family: 'wf_SegoeUILight';
    src:url('http://i.s-microsoft.com/fonts/Segoe-UI/Cyrillic/Light/latest.eot');
    src:url('http://i.s-microsoft.com/fonts/Segoe-UI/Cyrillic/Light/latest.eot?#iefix') format('embedded-opentype'),url('http://i.s-microsoft.com/fonts/Segoe-UI/Cyrillic/Light/latest.woff') format('woff'),url('http://i.s-microsoft.com/fonts/Segoe-UI/Cyrillic/Light/latest.ttf') format('truetype'),url('http://i.s-microsoft.com/fonts/Segoe-UI/Cyrillic/Light/latest.svg#web') format('svg');
    font-weight: normal;
    font-style: normal;
}
html {
	height: 100%;
}
body{
	margin: 0;padding: 0;
	background-image: url(img/1.jpg);
	min-width: 800px;
	height:100%;
	transition: 1s;
	transition-property: background;
}
body.busy{
	cursor: url(img/aero_busy.ani), wait;
}
ul, li {
	margin: 0; padding: 0;
	list-style: none;
}
#header_div {
	width: 100%; height: 40px;
} 
#header {
	position: absolute;
	top:0; left: 0;
	width: calc(100% - 13px);
	height: 40px;
	border-bottom: 1px solid rgba(255,255,255,0.3);
	box-sizing: border-box;
}
#header #menu_but { /*Кнопка меню*/
	position: absolute;
	width: 37px;
	height: 37px;
	margin: 1px 8px 0px 9px;
	background: url(img/menu.png);
	transition: 0.4s;
}
#header #menu_but:hover { /*Кнопка меню*/
	background: url(img/menu_act.png);
	transition: 1s;
	transition-property: background;
}
#header #menu_but.open {
	background: url(img/menu_act.png);
	transition: 1s;
	transition-property: background;
	z-index: 95;
}
#header #shortcuts {
	position: absolute;
	top: 0; left: 60px;
	height: 40px;
	overflow: hidden;
	-moz-user-select: none;
	-webkit-user-select: none;
	user-select: none; 
}
#header #shortcuts .shortcut {
	position: relative;
	width: 60px;
	height: 40px;
	padding: 3px 14px;
	box-sizing: border-box;
	border: 1px solid rgba(255,255,255,0.2);
	border-radius: 3px;
	background-repeat: no-repeat;
	background-color: rgba(255,255,255,0.1);
	background-image:
		-webkit-radial-gradient(100% 90%,ellipse,rgba(255,255,255,0) 69%,rgba(255,255,255,.2) 73%,rgba(255,255,255,.6) 100%),
		-webkit-linear-gradient(rgba(255,255,255,0.1) 0%, rgba(255,255,255,0.0) 50%);
	background-image:
		-webkit-radial-gradient(100% 90%,ellipse,rgba(255,255,255,0) 69%,rgba(255,255,255,.2) 73%,rgba(255,255,255,.6) 100%),
		-o-linear-gradient(rgba(255,255,255,0.1) 0%, rgba(255,255,255,0.0) 50%);
	background-image:
		-webkit-radial-gradient(100% 90%,ellipse,rgba(255,255,255,0) 69%,rgba(255,255,255,.2) 73%,rgba(255,255,255,.6) 100%),
		linear-gradient(rgba(255,255,255,0.1) 0%, rgba(255,255,255,0.0) 50%);;
}
#header #shortcuts .back_sc_grad{
	display: inline-block;
	margin: 0 2px;
	transition: 0.5s;
	transition-property: background;
}
#header #shortcuts .shortcut.open {
	background-color: rgba(255,255,255,0.3);
}
#header #tray {
	position: absolute;
	right: 77px;
	max-width: 80px;
	height: 100%;
}
#header #tray .tray {
	width: 20px;
	height: 40px;
	margin-right: 3px;
	border: 0;
	border-left: 1px solid transparent;
	border-right: 1px solid transparent;
	display: inline-block;
	box-sizing: border-box;
}
#header #tray .tray img {
	margin: 13px 2px;
	width: 14px;
	max-height: 14px;
}
#header #tray .tray:hover{
	-webkit-border-image: -webkit-linear-gradient(transparent 13%, rgba(255,255,255,0.3) 40%, rgba(255,255,255,0.3) 60%, transparent 87%) 1 stretch;
	-o-border-image: -o-linear-gradient(transparent 13%, rgba(255,255,255,0.3) 40%, rgba(255,255,255,0.3) 60%, transparent 87%) 1 stretch;
	border-image: linear-gradient(transparent 13%, rgba(255,255,255,0.3) 40%, rgba(255,255,255,0.3) 60%, transparent 87%) 1 stretch;
}
#header #tray .notification {
	width: 240px; height: 40px;
	position: absolute;
	top: 40px;
	right: -5px;
	padding: 9px;
	background: -webkit-linear-gradient(#fbfafd 0%, #e5e6f0 100%);
	background: -o-linear-gradient(#fbfafd 0%, #e5e6f0 100%);
	background: linear-gradient(#fbfafd 0%, #e5e6f0 100%);
	border: 1px solid #000;
	border-radius: 7px;
	display: inline-block;
	box-sizing: border-box;
	box-shadow: 3px 3px 3px rgba(0,0,0,0.3);
	z-index: 30;
	cursor: default;
}
#header #tray .notification:before {
	position: absolute;
	right: 15px;
	top: -21px;
	width: 0;
	height: 0;
	content: "";
	border-left: 20px solid transparent;
	border-bottom: 20px solid #000;
	box-shadow: 3px 1px 3px rgba(0,0,0,.3);
}
#header #tray .notification:after {
	position: absolute;
	right: 16px;
	top: -19px;
	width: 0;
	height: 0;
	content: "";
	border-left: 22px solid transparent;
	border-bottom: 22px solid #fbfafd;
}
#header #tray .notification .nclose {
	width: 9px;
	height: 8px;
	background: url(img/nclose.png);
	display: block;
	position: absolute;
	right: 8px; top: 8px;
}
#header #date_n_time{
	width: 75px; height: 40px;
	margin: 0; padding: 0;
	border: 0;
	border-left: 1px solid transparent;
	border-right: 1px solid transparent;
	position: absolute;
	top: 0px; right: 2px;
	text-align: center;
	box-sizing: border-box;
}
#header #date_n_time:hover{
	-webkit-border-image: -webkit-linear-gradient(transparent 13%, rgba(255,255,255,0.3) 40%, rgba(255,255,255,0.3) 60%, transparent 87%) 1 stretch;
	-o-border-image: -o-linear-gradient(transparent 13%, rgba(255,255,255,0.3) 40%, rgba(255,255,255,0.3) 60%, transparent 87%) 1 stretch;
	border-image: linear-gradient(transparent 13%, rgba(255,255,255,0.3) 40%, rgba(255,255,255,0.3) 60%, transparent 87%) 1 stretch;
}
#header #date_n_time p{
	color: white;
	margin: 2px 0;
	padding: 0;
	font-family: "wf_SegoeUINormal";
	font-size: 11.5px;
	line-height: 16px;
}
#header_bg_blur{ /*Размытие Aero*/
	position: absolute;
	top: 0; left: 0;
	width: 100%; height: 42px;
	box-shadow: 0 0 7px #000000;
	background: url(img/1.jpg);
	-webkit-filter: blur(3px);
	z-index: -1;
