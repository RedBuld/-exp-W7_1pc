#header_hide { /*Свернуть все окна*/
	position: absolute;
	top:0; right: 0;
	width: 13px;
	height: 40px;
	background: -webkit-radial-gradient(center,circle cover,rgba(255,255,255,0) 50%,rgba(255,255,255,.05) 50%,rgba(255,255,255,.4) 75%,rgba(255,255,255,.6) 100%);
	background-size: 270%;
	background-position: 0px -17px;
	background-repeat: no-repeat;
	/*farthest-corner*/
	border: 1px solid rgba(255,255,255,0.3);
	box-shadow: 0 -1px 0 1px rgba(0,0,0,0.4);
	box-sizing: border-box;
}
#header_hide:hover{ /*Свернуть все окна*/
	background: rgba(255,255,255,0.15) -webkit-radial-gradient(center,circle cover,rgba(255,255,255,0) 50%,rgba(255,255,255,.05) 50%,rgba(255,255,255,.4) 75%,rgba(255,255,255,.6) 100%);
	background-size: 270%;
	background-position: 0px -17px;
	background-repeat: no-repeat;
}
#menu {
	margin-top: 1px;
	width: 490px;
	background: rgba(0,0,0,0.4);
	border: 1px solid rgba(180,180,180,0.3);
	box-shadow: 0 0 0 1px rgba(0,0,0,0.6);
	border-radius: 10px 10px 0 0;
	position: absolute;
	z-index: 100;
	display: none;
	-moz-user-select: none;
	-webkit-user-select: none;
	user-select: none; 
}
#menu .main_block {
	width: 490px;
	height: 100%;
	position: relative;
	z-index: 10;
	font-family: "wf_SegoeUINormal";
	font-size: 13px;
	font-weight: 500;
}
#menu .main_block #left_mn_blc{
	width: 300px;
	margin: 6px;
	background: white;
	border-radius: 5px;
	border: 1px solid rgba(0,0,0,0.7);
	box-shadow: 0 0 3px rgba(255,255,255,0.4);
	box-sizing: border-box;
}
#menu .main_block #left_mn_blc #short_cats {
	height: 430px;
	overflow: hidden;
}
#menu .main_block #left_mn_blc #short_cats li {
	border:solid 1px transparent;
	display:block;
	padding:1px; margin:2px;
	text-decoration:none;
	min-width:220px;
	cursor: default;
}
#menu .main_block #left_mn_blc #short_cats li:hover {
	cursor: default;
	border:solid 1px #7da2ce;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	box-shadow: inset 0 0 1px #fff;
	-moz-box-shadow: inset 0 0 1px #fff;
	-webkit-box-shadow: inset 0 0 1px #fff;
	background-color:#cfe3fd;
	background: -moz-linear-gradient(top, rgba(227,238,252,1) 0%, rgba(204,226,252,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(227,238,252,1)), color-stop(100%,rgba(204,226,252,1)));
	background: -webkit-linear-gradient(top, rgba(227,238,252,1) 0%,rgba(204,226,252,1) 100%);
}
#menu .main_block #left_mn_blc #short_cats .icon img {
	border:0;
	vertical-align:middle;
	margin:0 5px 0 0;
}
#menu .main_block #left_mn_blc #all_cats {
	display: none;
	height: 430px;
	overflow-y: auto;
}
#menu .main_block #left_mn_blc #all_cats li {
	border:solid 1px transparent;
	display:block;
	padding:1px; margin:0 2px;
	text-decoration:none;
	min-width:220px;
	cursor: default;
}
#menu .main_block #left_mn_blc #all_cats li:hover {
	cursor: default;
	border:solid 1px #7da2ce;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	box-shadow: inset 0 0 1px #fff;
	-moz-box-shadow: inset 0 0 1px #fff;
	-webkit-box-shadow: inset 0 0 1px #fff;
	background-color:#cfe3fd;
	background: -moz-linear-gradient(top, rgba(227,238,252,1) 0%, rgba(204,226,252,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(227,238,252,1)), color-stop(100%,rgba(204,226,252,1)));
	background: -webkit-linear-gradient(top, rgba(227,238,252,1) 0%,rgba(204,226,252,1) 100%);
}
#menu .main_block #left_mn_blc #all_cats .icon img {
	border:0;
	vertical-align:middle;
	margin:0 3px 0 0;
}
#splitter {
	background: #d6e5f5;
	width: 230px; height: 1px;
	margin: 0px 35px 3px 35px;
}
#all_sh {
	border:solid 1px transparent;
	display:block;
	padding:0px; margin:3px 2px;
	text-decoration:none;
	min-width:220px;
	cursor: default;
}
#all_sh t{
	line-height: 12px;
	display: inline-block;
	vertical-align: super;
}
#all_sh .r_arrow {
	width: 32px; height: 22px;
	background-image: url(img/r_arrow.png);
	display: inline-block;
	transition: 0.3s;
}
#all_sh .l_arrow {
	width: 32px; height: 22px;
	background-image: url(img/l_arrow.png);
	display: inline-block;
	transition: 0.3s;
}
#all_sh:hover {
	cursor: default;
	border:solid 1px #7da2ce;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	box-shadow: inset 0 0 1px #fff;
	-moz-box-shadow: inset 0 0 1px #fff;
	-webkit-box-shadow: inset 0 0 1px #fff;
	background-color:#cfe3fd;
	background: -moz-linear-gradient(top, rgba(227,238,252,1) 0%, rgba(204,226,252,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(227,238,252,1)), color-stop(100%,rgba(204,226,252,1)));
	background: -webkit-linear-gradient(top, rgba(227,238,252,1) 0%,rgba(204,226,252,1) 100%);
}
#search_div {
	width: 298px;
	height: 40px;
	border: 0;
	box-shadow: inset 0 4px 6px -5px rgba(33, 65, 106, 0.6);
	background: rgba(153, 208, 231, .3);
	text-align: center;
	padding: 10px;
	box-sizing: border-box;
}
#search_div input {
	width: 270px;
	padding: 1px 4px;
	box-sizing: border-box;
	font-family: "wf_SegoeUINormal";
	font-style: italic;
	background-image: url(img/search.png);
	background-position: right center;
	background-repeat: no-repeat;
}
#search_div input:focus {
	box-sizing: border-box;
	font-style: normal;
}
#menu .main_block #right_mn_blc {
	width: 170px; height: 500px;
	position: absolute;
	right: 0; top: 0;
	margin: 6px;
}
#menu .main_block #right_mn_blc #div_icon {
	width: 100%; height: 64px;
	margin: 0 0 8px 0;
}
#menu .main_block #right_mn_blc .splitter, #right_mn_blc .splitter:hover{
	width: 130px !important;
	height: 1px !important;
	margin: 3px auto 1px auto !important; padding: 0 !important;
	background: rgba(255,255,255,0.1) !important;
	border: none; box-shadow: none !important;
}
#menu .main_block #div_icon .big_icon{
	width: 64px; height: 64px;
	margin: 0 auto;
}
#menu .main_block #div_icon .big_icon img {
	cursor: pointer;
}
#menu .main_block #right_mn_blc li {
	padding:5px 7px 7px 4px;
	display:block;
	font:normal 12px "wf_SegoeUINormal";
	color:#fff;
	text-decoration:none;
	margin-bottom:3px;
	border:1px solid transparent;
	cursor: default;
}
#menu .main_block #right_mn_blc li:hover {
	cursor: default;
	background:#222;
	background: -webkit-linear-gradient(bottom, rgba(255,255,255,.1) 0%, rgba(0,0,0,.0) 49%, rgba(255,255,255,.25) 51%, rgba(255,255,255,.35) 100%);
	background: -o-linear-gradient(bottom, rgba(255,255,255,.1) 0%, rgba(0,0,0,.0) 49%, rgba(255,255,255,.25) 51%, rgba(255,255,255,.35) 100%);
	background: linear-gradient(to top, rgba(255,255,255,.1) 0%, rgba(0,0,0,.0) 49%, rgba(255,255,255,.25) 51%, rgba(255,255,255,.35) 100%);
	border-color:#111;
	-webkit-box-shadow:0 0 1px rgba(255,255,255,.4),inset 0 0 1px 1px rgba(255,255,255,.3);
	-moz-box-shadow:0 0 1px rgba(255,255,255,.4),inset 0 0 1px 1px rgba(255,255,255,.3);
	box-shadow:0 0 1px rgba(255,255,255,.4),inset 0 0 1px 1px rgba(255,255,255,.3);
	-webkit-border-radius:4px;
	-moz-border-radius:4px;
	border-radius:4px;
}
#menu #right_mn_blc #start_order{
	max-width: 165px;
	height: 20px;
	position: absolute;
	bottom: 10px; left: 0;
}
#menu #right_mn_blc #start_order span{
	float: left;
	padding: 3px 5px;
	height: 21px;
	color: #fff;
	text-align: center;
	vertical-align: middle;
	border: 1px solid rgba(0,0,0,0.6);
	font-size: 12px; line-height: 12px; letter-spacing: 0.3px;
	cursor: default;
	background: -webkit-linear-gradient(bottom, rgba(255,255,255,.0) 0%, rgba(0,0,0,.0) 49%, rgba(255,255,255,.25) 51%, rgba(255,255,255,.35) 100%);
	background: -o-linear-gradient(bottom, rgba(255,255,255,.0) 0%, rgba(0,0,0,.0) 49%, rgba(255,255,255,.25) 51%, rgba(255,255,255,.35) 100%);
